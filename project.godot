; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "CutsceneRuntime",
"language": "GDScript",
"path": "res://addons/GViNT/Core/Runtime/CutsceneRuntime.gd"
}, {
"base": "Reference",
"class": "GvintContext",
"language": "GDScript",
"path": "res://addons/GViNT/Core/Context.gd"
}, {
"base": "Reference",
"class": "GvintVariable",
"language": "GDScript",
"path": "res://addons/GViNT/Core/Variable.gd"
}, {
"base": "Node",
"class": "VNRuntime",
"language": "GDScript",
"path": "res://addons/GViNT/Core/Runtime/VNRuntime.gd"
} ]
_global_script_class_icons={
"CutsceneRuntime": "",
"GvintContext": "",
"GvintVariable": "",
"VNRuntime": ""
}

[application]

config/name="GViNT"
config/icon="res://icon.png"

[autoload]

GvintScripts="*res://addons/GViNT/Autoload/GvintScripts.tscn"

[debug]

gdscript/warnings/exclude_addons=false
gdscript/warnings/unused_variable=false
gdscript/warnings/unused_argument=false
gdscript/warnings/unused_signal=false
gdscript/warnings/return_value_discarded=false

[editor_plugins]

enabled=PoolStringArray( "res://addons/GViNT/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
